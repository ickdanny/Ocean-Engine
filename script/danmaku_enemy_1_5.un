# bird dropping danmaku

let angleRange = 10.0;
let speed = 35.0;

func spawnBullet(vel, depthOffset){
    spawn(
        "medium_clear",
        getPosition(),
        vel,
        depthOffset
    )
}

let angle = 90.0 + random(-angleRange, angleRange);
let velocity = <<speed, angle>>;
spawnBullet(velocity, 0);